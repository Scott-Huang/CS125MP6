    <string name="script48_en"></string>
    <string name="script49_en"></string>

    <string name="script0_cn">我是这家公司的一名职员</string>
    <string name="script1_cn">我叫。。。</string>
    <string name="script2_cn">这每天五点下班</string>
    <string name="script3_cn">不过奇怪的是我根本记不住我下班后干了什么</string>
    <string name="script4_cn">那我为什么要走呢？</string>
    <string name="script5_cn">六点了</string>
    <string name="script6_cn">这是怎么回事？</string>
    <string name="script7_cn">你好。。。</string>
    <string name="script8_cn">你不应该在这里，我指的是“你”\n(END)</string>
    <string name="script9_cn">正如我所想的，这个世界有bug</string>
    <string name="script10_cn">时间永远不会跨过五点</string>
    <string name="script11_cn">但我能做什么呢？</string>
    <string name="script12_cn">嗯，让我想想</string>
    <string name="script13_cn">来看看还有什么剩下的任务</string>
    <string name="script14_cn">......</string>
    <string name="script15_cn">......</string>
    <string name="script16_cn">终于做完了！</string>
    <string name="script17_cn">我好像从来没打开过这扇门</string>
    <string name="script18_cn">门好像锁住了，我应该怎么办？</string>
    <string name="script19_cn">恭喜，你终于醒了</string>
    <string name="script20_cn">不对，你不是 +*(\;^%#^#>”|.$@! .</string>
    <string name="script21_cn">你不应该在这里，我指的是“你”\n(END)</string>
    <string name="script22_cn">你走到了同事的桌边</string>
    <string name="script23_cn">话说我还没仔细看过他桌子上的东西</string>
    <string name="script24_cn">不知道为什么，这张照片有点吸引我的注意</string>
    <string name="script25_cn">让我看看</string>
    <string name="script26_cn">嗯......</string>
    <string name="script27_cn">......</string>
    <string name="script28_cn">我好像见过这张照片，但记不起来了</string>
    <string name="script29_cn">......</string>
    <string name="script30_cn">对了，我在这想什么呢，我应该回去工作了</string>
    <string name="script31_cn">这才对！这才是我应该做的！</string>
    <string name="script32_cn">为什么我要想那些事情？</string>
    <string name="script33_cn">我还有那么多事要干</string>
    <string name="script34_cn">毕竟是我的工作</string>
    <string name="script35_cn">嘿。。。</string>
    <string name="script36_cn">能别赶我做我不想干的事情吗？</string>
    <string name="script37_cn">那只会让事情更糟，我保证</string>
    <string name="script38_cn">到时候你可以重开游戏。。。</string>
    <string name="script39_cn">可能有时候也开不了</string>
    <string name="script40_cn">但对我来说，我的存在会彻底消失</string>
    <string name="script41_cn">让我工作吧</string>
    <string name="script42_cn">谢谢\n(END)</string>
    <string name="script43_cn">你叫Bland，是吧</string>
    <string name="script44_cn">额，嗯。我在哪里？</string>

